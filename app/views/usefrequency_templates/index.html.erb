<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All usefrequencies
    </h1>

    <a href="/usefrequencies/new" class="btn btn-block btn-outline-secondary">
      Add a new usefrequency
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Description
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @usefrequencies.each do |usefrequency| %>
      <tr>
        <!-- Display usefrequency.id start -->
        <td>
          <%= usefrequency.id %>
        </td>
        <!-- Display usefrequency.id end -->

        <!-- Display usefrequency.description start -->
        <td>
          <%= usefrequency.description %>
        </td>
        <!-- Display usefrequency.description end -->

        <!-- Display usefrequency.created_at start -->
        <td>
          <%= time_ago_in_words(usefrequency.created_at) %> ago
        </td>
        <!-- Display usefrequency.created_at end -->

        <!-- Display usefrequency.updated_at start -->
        <td>
          <%= time_ago_in_words(usefrequency.updated_at) %> ago
        </td>
        <!-- Display usefrequency.updated_at end -->

        <td>
          <a href="/usefrequencies/<%= usefrequency.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @usefrequencies, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
